{
    "version": 2,
    "builds": [
        {
            "src": "stock/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/:path((?!api/|.*\\.).*)",
            "destination": "/index.html"
        }
    ]
}
