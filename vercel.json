{
  "version": 2,
  "builds": [
    {
      "src": "stock/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!api/|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
